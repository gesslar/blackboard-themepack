{
  "name": "blackboard-themepack",
  "displayName": "Blackboard Theme Pack",
  "description": "Classic blackboard aesthetics for night coding - chalk on slate with electric blue accents",
  "version": "1.0.0",
  "publisher": "gesslar",
  "license": "Unlicense",
  "engines": {
    "vscode": ">=1.50.0"
  },
  "homepage": "https://sassy.gesslar.io",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "watch": "pnpx @gesslar/sassy build -wo ./dist ./src/*.yaml",
    "build": "pnpx @gesslar/sassy build -o ./dist ./src/*.yaml",
    "lint-blackboard": "pnpx @gesslar/sassy lint ./src/blackboard.yaml",
    "package": "pnpx @gesslar/sassy build -o ./dist ./src/*.yaml && pnpm dlx @vscode/vsce package --no-dependencies --skip-license",
    "publish:vsce": "pnpm run package && xdg-open https://marketplace.visualstudio.com/manage/publishers/gesslar",
    "publish:ovsx": "pnpm run package && xdg-open https://open-vsx.org/user-settings/extensions",
    "update": "pnpm self-update && pnpx npm-check-updates -u && pnpm install",
    "pr": "gt submit -p --ai",
    "patch": "pnpm version patch",
    "minor": "pnpm version minor",
    "major": "pnpm version major"
  },
  "categories": [
    "Themes"
  ],
  "type": "module",
  "contributes": {
    "themes": [
      {
        "label": "Blackboard",
        "description": "Don't use your nails, eek!",
        "id": "blackboard",
        "uiTheme": "vs-dark",
        "path": "dist/blackboard.color-theme.json"
      },
      {
        "label": "Bubblegum Goth",
        "description": "*POP* goes the emo",
        "id": "bubblegum-goth",
        "uiTheme": "vs-dark",
        "path": "dist/bubblegum-goth.color-theme.json"
      },
      {
        "label": "Corporate",
        "description": "They'll definitely think you're working now.",
        "id": "corporate",
        "uiTheme": "vs-dark",
        "path": "dist/corporate.color-theme.json"
      }
    ]
  },
  "icon": "media/blackboard.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gesslar/blackboard-theme"
  },
  "keywords": [
    "theme",
    "blackboard",
    "dark",
    "chalk",
    "night",
    "blue",
    "pink",
    "sassy"
  ],
  "__metadata": {
    "id": "blackboard-theme"
  },
  "devDependencies": {
    "@gesslar/uglier": "^1.2.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.2",
    "ovsx": "^0.10.8"
  }
}
